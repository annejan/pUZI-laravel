{
    "name": "minvws/puzi-laravel",
    "description": "Laravel authentication middleware for UZI passes.",
    "type": "laravel",
    "license": "EUPL",
    "keywords": [
      "security",
      "crypto",
      "cryptography",
      "signature",
      "x509",
      "x.509",
      "asn1",
      "asn.1",
      "UZI"
    ],
    "autoload": {
        "psr-4": { "MinVWS\\PUZI\\Laravel\\": "src" }
    },
    "autoload-dev": {
      "psr-4": {
        "MinVWS\\PUZI\\Laravel\\Tests\\": "tests/"
      }
    },
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "minvws/puzi-php": "^0.4",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "vimeo/psalm": "^4.4",
      "phpstan/phpstan": "^0.12.80",
      "squizlabs/php_codesniffer": "^3.5",
      "orchestra/testbench": "^6.13",
      "mockery/mockery": "^1.4"
    },
    "authors": [
      {
        "name": "Anne Jan Brouwer",
        "email": "brouwer@annejan.com"
      },
      {
        "name": "Nick ten Cate",
        "email": "nick@nicktencate.com"
      },
      {
        "name": "Mendel Mobach",
        "email": "mendel@mobach.nl"
      },
      {
        "name": "Joshua Thijssen",
        "email": "jthijssen@noxlogic.nl"
      }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MinVWS\\Laravel\\PUZI\\UziServiceProvider"
            ]
        }
    },
    "scripts": {
      "test": [
          "vendor/bin/phpunit",
          "vendor/bin/phpstan analyse",
          "vendor/bin/psalm",
          "vendor/bin/phpcs"
      ]
    }
}
